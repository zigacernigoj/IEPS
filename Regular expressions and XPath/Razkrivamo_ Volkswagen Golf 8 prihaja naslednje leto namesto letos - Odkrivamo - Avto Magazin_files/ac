	var sas_appendToContainer = function(id,elem){
        var tagId=document.getElementById(id);
        if ('string' == typeof elem){
            var div = document.createElement('div');
            div.innerHTML = elem;
            elem = div;
        }
        tagId.appendChild(elem);    
    };

	if (isAsync) {
		var js = document.createElement('script'); js.src = lib;
		sas_appendToContainer('sas_29462', js);
