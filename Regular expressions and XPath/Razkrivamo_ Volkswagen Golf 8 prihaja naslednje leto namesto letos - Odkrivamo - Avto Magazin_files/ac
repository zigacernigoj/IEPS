			var ifr = document.createElement('iframe');
			ifr.src = 'about:blank';
			var css = 'position:absolute;left:-1000px;width:1px;height:1px';
			ifr.setAttribute('style', css);
			ifr.setAttribute('id', 'sas_iframe_8643399');
			ifr.style.cssText = css;
			sas_appendToContainer('sas_29462', ifr);
			var d = ifr.contentWindow.document;
			d.open('text/html', 'replace');
			d.write('<!DOCTYPE html><head></head><body>' + scr + '</body></html>');
			d.close();
		}
	} else
		document.write('<scr'+'ipt src="'+lib+'"></scr'+'ipt>' + scr);
